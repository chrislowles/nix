{
    "additional-repositories": ["multilib"],
    "archinstall-language": "en",
    "audio_config": "pipewire",
    "bootloader": "systemd-boot",
    "hostname": "changethisplease",
    "kernels": ["linux"],
    "locale_config": {
        "kb_layout": "us",
        "sys_enc": "UTF-8",
        "sys_lang": "en_US"
    },
    "mirror_config": {
        "custom_mirrors": [],
        "mirror_regions": {
            "Worldwide": [
                "https://geo.mirror.pkgbuild.com/$repo/os/$arch"
            ]
        }
    },
    "network_config": {
        "type": "nm"
    },
    "no_pkg_lookups": false,
    "ntp": true,
    "offline": false,
    "packages": [
        "bash", "bash-completion", "gvim", "git", "jre-openjdk", "flatpak", "wget", "curl", "python", "python-pipx",
        "plasma-meta", "plasma-nm", "plasma-pa", "dolphin", "konsole", "kde-gtk-config", "plasma-systemmonitor", "breeze-gtk",
        "sddm", "sddm-kcm",
        "yt-dlp",
        "firefox",
        "zed", "flatpak",
        "wine-gecko", "wine-mono", "wine-staging"
    ],
    "parallel downloads": 25,
    "script": "guided",
    "swap": true,
    "services": ["sddm"],
    "timezone": "Australia/Sydney",
    "version": "2.8.6"
}